/// Summary value.

/// Summary value type.
(smVal) [. if];

/// Constructor.
///   ix : Index in results array.
///   val: Value.
//: [. if]
new : ix, val;

/// Update values of 'this'.
//: [(smVal)if|]
up = \this, newIx, newVal -> {
  this[ix] = newIx;
  this[val] = newVal;
};

//: [(smVal)|s]
toJs = \o -> return js.wa([
    js.wi(o[ix]),
    js.wf(o[val])
  ]);;
