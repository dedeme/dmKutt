/// Historic quote.

import "libmkt/quote";

/// 'Historic quote' type.
(historicQ) [. sfffff];

/// Constructor.
/// Values <= 0 are not valid ones.
///   date : Quote date.
///   open : Open value.
///   close: Close value.
///   max  : Maximum value.
///   min  : Minimum value.
///   vol  : Volume value.
/// \s,f,f,f,f,f -> <historicQ>
//: [. sfffff]
new : date, open, close, max, min, vol;

/// Returns a <quote> with the field 'force' set to false.
/// \<historicQ> -> <quote>
//: [(historicQ)|(quote)]
toQuote = \q -> return quote.new(
  q[date], q[open], q[close], q[max], q[min], math.ftoi(q[vol]), false
);;

//: [s|(historicQ)]
fromJs = \j -> {
  A = js.ra(j);
  return [.
    js.rs(A[date]),
    js.rf(A[open]),
    js.rf(A[close]),
    js.rf(A[max]),
    js.rf(A[min]),
    js.rf(A[vol])
  ];
};
