/// Current quote value.

/// 'Current quote value' type
(currentQ) [. sf];

/// Constructor.
/// Value <= 0 is not valid.
///   code : Company server code.
///   value: Current value.
//: [. sf]
new : code, value;

//: [s|(currentQ)]
fromJs = \j -> {
  A = js.ra(j);
  return [.
    js.rs(A[code]),
    js.rf(A[value])
  ];
};
