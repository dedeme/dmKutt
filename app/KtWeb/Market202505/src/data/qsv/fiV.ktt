/// Last fixed income value

/// 'Last fixed income value' type
(fiV) [. sf];

/// Constructor.
///   date: Value date.
///   irr : internal ratio of return.
//: [. sf]
new : date, irr;

//: [s|(fiV)]
fromJs = \j -> {
  A = js.ra(j);
  return [.
    js.rs(A[date]),
    js.rf(A[irr])
  ];
};
