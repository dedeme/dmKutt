/// Trading data base.

import "trading/closesTb";
import "trading/refsTb";
import "cts";

/// Data base file.
//: [|s]
dpath = \ -> return file.cat(cts.dataPath(), ["trading"]);;

/// Initializes data base.
//: [|]
init = \ -> {
  file.mkdir(dpath());
  closesTb.init();
  refsTb.init();
};
