/// Daily charts data base.

import "daily/cosSelTb";
import "daily/dailyChartsTb";
import "cts";

/// Data base file.
//: [|s]
dpath = \ -> return file.cat(cts.dataPath(), ["daily"]);;

/// Initializes data base.
//: [|]
init = \ -> {
  file.mkdir(dpath());
  cosSelTb.init();
  dailyChartsTb.init();
};
