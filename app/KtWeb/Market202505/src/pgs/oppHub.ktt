/// Oportunity costs hub.

import "libdm/svrp";
import "opp/chartsPg";
import "opp/listPg";

//: [{s}|s]
process = \Rq -> {
  source = js.rs(Rq\source);
  return switch (source) {
    "ChartsPg": chartsPg.process(Rq);
    "ListPg": listPg.process(Rq);
    default: throw([]/s/, "Value of source (" + source + ") is not valid");
  };
};
