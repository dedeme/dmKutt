/// Home page.

import "libdm/svrp";
import "libdm/log";
import "cts";
import "db";

//: [{s}|s]
process = \:dic Rq -> {
  rq = js.rs(Rq\rq);
  switch (rq) {
    "getLog": return svrp.mk({
        Log: log.readJs()
      });
    "resetLog": {
        log.reset();
        return svrp.mkEmpty();
      }
    default: throw "Value of rq (" + rq + ") is not valid";
  }
};

