/// Home page.

import "libdm/log";
import "db";

//: [{s}|s]
process = \Rq -> {
  rq = js.rs(Rq\rq);
  switch (rq) {
    "getLog": return js.wo({
        Log: log.readJs()
      });
    "resetLog": {
        log.reset();
        return "{}";
      }
  }
  throw "Value of rq (" + rq + ") is not valid";
};
