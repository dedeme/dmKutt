/// Company nicks reader.

:file fpath = "/dm/KtWeb/dmcgi/Market/cos.tb";

/// Returns selected companies sorted by ascendent alphabetic order.
//: [|[s]]
read = \ -> {
  :arr Cos = arr.map(
    arr.filter(
      js.ra(js.ra(fpath.read())[1]), \E -> return js.rb(js.ra(E)[1]);
    ), \E -> return js.rs(js.ra(E)[0]);
  );
  Cos.sort(\c1, c2 -> return c1 < c2;);
  return Cos;
};
