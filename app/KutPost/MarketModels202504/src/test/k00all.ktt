import "k01test";
import "k02quotes";
import "k03volume";
import "k04refs";
import "k05stSimple";
import "k06stSimple2";
import "k07stGroup";
import "k08stGroup2";
import "k09stOpen";

/// All tests.

//: [s|s]
process = \rq -> {
  Rq = js.ro(rq);
  type = js.rs(Rq\type);
  return js.wo(switch (type) {
    "test": k01test.run(Rq);
    "quotes": k02quotes.run(Rq);
    "volume": k03volume.run(Rq);
    "refs": k04refs.run(Rq);
    "stSimple": k05stSimple.run(Rq);
    "stSimple2": k06stSimple2.run(Rq);
    "stGroup": k07stGroup.run(Rq);
    "stGroup2": k08stGroup2.run(Rq);
    "stOpen": k09stOpen.run(Rq);
    default: throw([]/{s}/, "Test type '" + type + "' is not valid.");
  });
};
