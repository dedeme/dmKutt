/// Test volumes.

import "libmkt/volumes";
import "extDb/cosTb";
import "cts";

/// Returns
///   {
///     vol: [i.]
///   }
//: [{s}|{s}]
run = \Rq -> return {
    vol: js.wa(arr.map(
        volumes.read(cts.quotesPath, 60, cosTb.read()),
        \v -> return js.wi(math.ftoi(v));
      ))
  };;
